---
import MainLayout from "@/layouts/MainLayout.astro";
import { SITE } from "@/data_files/constants";

const pageTitle: string = `SEO Services | ${SITE.title}`;
const metaDescription = "Be seen, get found, stay ahead. Our expert SEO strategies help businesses improve search rankings, drive organic traffic, and dominate their market online.";
const ogTitle = "SEO Services | Trade Guild Consulting";
---

<MainLayout
  title={pageTitle}
  customDescription={metaDescription}
  customOgTitle={ogTitle}
  structuredData={{
    "@context": "https://schema.org",
    "@type": "Service",
    "@id": "https://tradeguildconsulting.com/services/seo-services",
    url: "https://tradeguildconsulting.com/services/seo-services",
    name: "SEO Services",
    description: metaDescription,
    provider: {
      "@type": "Organization",
      name: "Trade Guild Consulting",
      url: "https://tradeguildconsulting.com"
    },
    serviceType: "Search Engine Optimization",
    areaServed: "Worldwide"
  }}
>
  <!-- Breadcrumb Navigation -->
  <nav class="mx-auto max-w-[85rem] px-4 pt-6 pb-2 sm:px-6 lg:px-8" aria-label="Breadcrumb">
    <ol class="flex items-center space-x-2 text-sm text-gray-600 dark:text-neutral-400">
      <li>
        <a href="/" class="hover:text-[#FE9002] transition-colors">Home</a>
      </li>
      <li class="flex items-center">
        <svg class="w-4 h-4 mx-2" fill="currentColor" viewBox="0 0 20 20">
          <path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
        </svg>
        <a href="/services" class="hover:text-[#FE9002] transition-colors">Services</a>
      </li>
      <li class="flex items-center">
        <svg class="w-4 h-4 mx-2" fill="currentColor" viewBox="0 0 20 20">
          <path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
        </svg>
        <span class="text-gray-800 dark:text-neutral-200 font-medium">SEO Services</span>
      </li>
    </ol>
  </nav>

  <!-- Hero Section -->
  <section class="mx-auto max-w-[85rem] px-4 py-16 sm:px-6 lg:px-8 lg:py-20 2xl:max-w-full">
    <div class="relative rounded-3xl mx-4 sm:mx-6 lg:mx-8 overflow-hidden">
      <!-- Background -->
      <div class="absolute inset-0 bg-gradient-to-br from-gray-50 via-orange-50/30 to-yellow-50/20 dark:from-neutral-900 dark:via-neutral-900/95 dark:to-neutral-800/90"></div>
      
      <!-- Decorative Elements -->
      <div class="absolute top-20 left-10 w-20 h-20 bg-gradient-to-br from-[#D89D40]/10 to-[#FE9002]/10 rounded-full blur-xl animate-pulse"></div>
      <div class="absolute bottom-20 right-10 w-32 h-32 bg-gradient-to-br from-[#FE9002]/10 to-[#D89D40]/10 rounded-full blur-xl animate-pulse" style="animation-delay: 2s;"></div>

      <div class="relative z-10 mx-auto max-w-4xl text-center px-4 py-16 sm:px-6 lg:px-8 lg:py-20">
      <!-- Icon -->
      <div class="inline-flex items-center justify-center w-20 h-20 bg-gradient-to-br from-[#D89D40] to-[#FE9002] rounded-2xl mb-8 shadow-lg">
        <svg class="w-10 h-10 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"></path>
        </svg>
      </div>

      <!-- Headline -->
      <h1 class="text-4xl font-bold text-neutral-800 dark:text-neutral-100 lg:text-6xl mb-6">
        Be Seen, Get Found, Stay Ahead.
      </h1>
      
      <!-- Decorative Line -->
      <div class="w-32 h-1 bg-gradient-to-r from-[#D89D40] to-[#FE9002] mx-auto mb-8 rounded-full"></div>
      
      <!-- Subheadline -->
      <p class="text-xl text-neutral-600 dark:text-neutral-300 max-w-3xl mx-auto leading-relaxed mb-10">
        In today's digital world, being found online isn't optional—it's essential. Our SEO strategies don't just improve your search rankings; they position your business as the go-to authority in your industry.
      </p>

      <!-- CTA Button -->
      <a
        href="/contact"
        class="inline-flex justify-center items-center gap-x-3 text-center bg-gradient-to-r from-[#D89D40] to-[#FE9002] hover:from-[#c8863a] hover:to-[#e8820a] border border-transparent text-white text-lg font-semibold rounded-xl focus:outline-none focus:ring-2 focus:ring-[#FE9002] focus:ring-offset-2 focus:ring-offset-white transition-all duration-300 py-4 px-8 shadow-lg hover:shadow-xl hover:shadow-[#D89D40]/25 dark:hover:shadow-[#FE9002]/25 hover:scale-105 dark:focus:ring-offset-gray-800"
      >
        Dominate Your Market Online
        <svg class="flex-shrink-0 w-5 h-5" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="m9 18 6-6-6-6"/></svg>
      </a>
      </div>
    </div>
  </section>

  <!-- Why Choose Section -->
  <section class="mx-auto max-w-[85rem] px-4 py-16 sm:px-6 lg:px-8 lg:py-20 2xl:max-w-full">
    <div class="mx-auto max-w-4xl">
      <!-- Section Header -->
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-neutral-800 dark:text-neutral-200 lg:text-4xl mb-4">
          Why Choose Trade Guild Consulting for SEO?
        </h2>
        <div class="w-24 h-1 bg-gradient-to-r from-[#D89D40] to-[#FE9002] mx-auto mb-6 rounded-full"></div>
      </div>

      <!-- Content -->
      <div class="bg-gradient-to-br from-[#D89D40]/5 via-orange-50/50 to-[#FE9002]/5 dark:from-[#FE9002]/5 dark:via-neutral-700/30 dark:to-[#D89D40]/5 rounded-2xl p-8 border border-[#D89D40]/20 dark:border-[#FE9002]/20">
        <p class="text-lg text-neutral-700 dark:text-neutral-300 leading-relaxed text-center">
          Because we understand that SEO isn't just about rankings—it's about results. We combine technical expertise with strategic thinking to deliver SEO campaigns that drive qualified traffic, generate leads, and grow your business.
        </p>
      </div>
    </div>
  </section>

  <!-- Services Section -->
  <section class="mx-auto max-w-[85rem] px-4 py-16 sm:px-6 lg:px-8 lg:py-20 2xl:max-w-full">
    <div class="mx-auto max-w-6xl">
      <!-- Section Header -->
      <div class="text-center mb-16">
        <h2 class="text-3xl font-bold text-neutral-800 dark:text-neutral-200 lg:text-4xl mb-4">
          Our SEO Services Include:
        </h2>
        <div class="w-24 h-1 bg-gradient-to-r from-[#D89D40] to-[#FE9002] mx-auto mb-6 rounded-full"></div>
      </div>

      <!-- Services Grid -->
      <div class="grid gap-8 md:grid-cols-2 lg:grid-cols-3">
        <!-- Technical SEO -->
        <div class="bg-white dark:bg-neutral-800 rounded-xl p-6 shadow-sm border border-gray-200 dark:border-neutral-700 hover:shadow-lg hover:border-[#FE9002]/20 dark:hover:border-[#FE9002]/20 transition-all duration-300">
          <div class="w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] rounded-xl flex items-center justify-center mb-4">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z"></path>
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"></path>
            </svg>
          </div>
          <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Technical SEO</h3>
          <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
            Optimize your website's technical foundation with site speed improvements, mobile optimization, and crawlability enhancements.
          </p>
        </div>

        <!-- Keyword Research & Strategy -->
        <div class="bg-white dark:bg-neutral-800 rounded-xl p-6 shadow-sm border border-gray-200 dark:border-neutral-700 hover:shadow-lg hover:border-[#FE9002]/20 dark:hover:border-[#FE9002]/20 transition-all duration-300">
          <div class="w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] rounded-xl flex items-center justify-center mb-4">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 15l-2 5L9 9l11 4-5 2zm0 0l5 5M7.188 2.239l.777 2.897M5.136 7.965l-2.898-.777M13.95 4.05l-2.122 2.122m-5.657 5.656l-2.12 2.122"></path>
            </svg>
          </div>
          <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Keyword Research & Strategy</h3>
          <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
            Identify high-value keywords and develop comprehensive strategies to target the terms your customers are actually searching for.
          </p>
        </div>

        <!-- On-Page Optimization -->
        <div class="bg-white dark:bg-neutral-800 rounded-xl p-6 shadow-sm border border-gray-200 dark:border-neutral-700 hover:shadow-lg hover:border-[#FE9002]/20 dark:hover:border-[#FE9002]/20 transition-all duration-300">
          <div class="w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] rounded-xl flex items-center justify-center mb-4">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z"></path>
            </svg>
          </div>
          <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">On-Page Optimization</h3>
          <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
            Optimize your content, meta tags, headers, and internal linking structure to maximize search engine visibility and user experience.
          </p>
        </div>

        <!-- Content Strategy -->
        <div class="bg-white dark:bg-neutral-800 rounded-xl p-6 shadow-sm border border-gray-200 dark:border-neutral-700 hover:shadow-lg hover:border-[#FE9002]/20 dark:hover:border-[#FE9002]/20 transition-all duration-300">
          <div class="w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] rounded-xl flex items-center justify-center mb-4">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
            </svg>
          </div>
          <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Content Strategy</h3>
          <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
            Develop SEO-optimized content that engages your audience while targeting valuable keywords and search intent.
          </p>
        </div>

        <!-- Link Building -->
        <div class="bg-white dark:bg-neutral-800 rounded-xl p-6 shadow-sm border border-gray-200 dark:border-neutral-700 hover:shadow-lg hover:border-[#FE9002]/20 dark:hover:border-[#FE9002]/20 transition-all duration-300">
          <div class="w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] rounded-xl flex items-center justify-center mb-4">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"></path>
            </svg>
          </div>
          <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Link Building</h3>
          <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
            Build high-quality backlinks from authoritative websites to increase your domain authority and search rankings.
          </p>
        </div>

        <!-- Local SEO -->
        <div class="bg-white dark:bg-neutral-800 rounded-xl p-6 shadow-sm border border-gray-200 dark:border-neutral-700 hover:shadow-lg hover:border-[#FE9002]/20 dark:hover:border-[#FE9002]/20 transition-all duration-300">
          <div class="w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] rounded-xl flex items-center justify-center mb-4">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
            </svg>
          </div>
          <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Local SEO</h3>
          <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
            Dominate local search results with Google My Business optimization, local citations, and location-based content strategies.
          </p>
        </div>
      </div>
    </div>
  </section>

  <!-- Our Approach Section -->
  <section class="mx-auto max-w-[85rem] px-4 py-16 sm:px-6 lg:px-8 lg:py-20 2xl:max-w-full">
    <div class="bg-gray-50 dark:bg-neutral-900 rounded-3xl mx-4 sm:mx-6 lg:mx-8 relative overflow-hidden" style="background-image: radial-gradient(circle at 1px 1px, rgba(255,255,255,0.15) 1px, transparent 0); background-size: 20px 20px;">
      <div class="absolute inset-0 bg-gradient-to-br from-neutral-900/5 to-neutral-800/10 dark:from-neutral-800/20 dark:to-neutral-700/30"></div>
      <div class="relative z-10 px-4 py-16 sm:px-6 lg:px-8 lg:py-20">
    <div class="mx-auto max-w-4xl">
      <!-- Section Header -->
      <div class="text-center mb-16">
        <h2 class="text-3xl font-bold text-neutral-800 dark:text-neutral-200 lg:text-4xl mb-4">
          Our SEO Approach
        </h2>
        <div class="w-24 h-1 bg-gradient-to-r from-[#D89D40] to-[#FE9002] mx-auto mb-6 rounded-full"></div>
      </div>

      <!-- Approach Steps -->
      <div class="space-y-8">
        <!-- Step 1 -->
        <div class="flex items-start">
          <div class="shrink-0 w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] text-white rounded-xl flex items-center justify-center font-bold text-lg mr-6 shadow-lg">
            1
          </div>
          <div class="flex-1 bg-white dark:bg-neutral-800 rounded-xl p-6 border border-gray-200 dark:border-neutral-700">
            <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Comprehensive SEO Audit</h3>
            <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
              We start with a thorough analysis of your current SEO performance, identifying opportunities and areas for improvement.
            </p>
          </div>
        </div>

        <!-- Step 2 -->
        <div class="flex items-start">
          <div class="shrink-0 w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] text-white rounded-xl flex items-center justify-center font-bold text-lg mr-6 shadow-lg">
            2
          </div>
          <div class="flex-1 bg-white dark:bg-neutral-800 rounded-xl p-6 border border-gray-200 dark:border-neutral-700">
            <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Strategic Planning</h3>
            <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
              We develop a customized SEO strategy based on your business goals, target audience, and competitive landscape.
            </p>
          </div>
        </div>

        <!-- Step 3 -->
        <div class="flex items-start">
          <div class="shrink-0 w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] text-white rounded-xl flex items-center justify-center font-bold text-lg mr-6 shadow-lg">
            3
          </div>
          <div class="flex-1 bg-white dark:bg-neutral-800 rounded-xl p-6 border border-gray-200 dark:border-neutral-700">
            <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Implementation & Optimization</h3>
            <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
              We execute the strategy with precision, implementing technical improvements, content optimization, and link building campaigns.
            </p>
          </div>
        </div>

        <!-- Step 4 -->
        <div class="flex items-start">
          <div class="shrink-0 w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] text-white rounded-xl flex items-center justify-center font-bold text-lg mr-6 shadow-lg">
            4
          </div>
          <div class="flex-1 bg-white dark:bg-neutral-800 rounded-xl p-6 border border-gray-200 dark:border-neutral-700">
            <h3 class="text-xl font-bold text-neutral-800 dark:text-white mb-3">Monitoring & Reporting</h3>
            <p class="text-neutral-600 dark:text-neutral-400 leading-relaxed">
              We continuously monitor your SEO performance and provide detailed reports showing rankings, traffic, and ROI improvements.
            </p>
          </div>
        </div>
      </div>
      </div>
    </div>
  </section>

  <!-- Why It Works Section -->
  <section class="mx-auto max-w-[85rem] px-4 py-16 sm:px-6 lg:px-8 lg:py-20 2xl:max-w-full">
    <div class="mx-auto max-w-4xl text-center">
      <h2 class="text-3xl font-bold text-neutral-800 dark:text-neutral-200 lg:text-4xl mb-6">
        Why Our SEO Works
      </h2>
      <div class="w-24 h-1 bg-gradient-to-r from-[#D89D40] to-[#FE9002] mx-auto mb-8 rounded-full"></div>
      <p class="text-lg text-neutral-600 dark:text-neutral-400 leading-relaxed mb-10">
        We don't chase algorithm updates—we focus on fundamental SEO principles that have stood the test of time. Our approach combines technical expertise with strategic thinking to deliver sustainable, long-term results.
      </p>

      <!-- Final CTA -->
      <a
        href="/contact"
        class="inline-flex justify-center items-center gap-x-3 text-center bg-gradient-to-r from-[#D89D40] to-[#FE9002] hover:from-[#c8863a] hover:to-[#e8820a] border border-transparent text-white text-lg font-semibold rounded-xl focus:outline-none focus:ring-2 focus:ring-[#FE9002] focus:ring-offset-2 focus:ring-offset-white transition-all duration-300 py-4 px-8 shadow-lg hover:shadow-xl hover:shadow-[#D89D40]/25 dark:hover:shadow-[#FE9002]/25 hover:scale-105 dark:focus:ring-offset-gray-800"
      >
        Dominate Your Market Online
        <svg class="flex-shrink-0 w-5 h-5" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="m9 18 6-6-6-6"/></svg>
      </a>
    </div>
  </section>

  <!-- Related Services -->
  <section class="mx-auto max-w-[85rem] px-4 py-16 sm:px-6 lg:px-8 lg:py-20 2xl:max-w-full mb-16">
    <div class="bg-gray-50 dark:bg-neutral-900 rounded-3xl mx-4 sm:mx-6 lg:mx-8 relative overflow-hidden" style="background-image: radial-gradient(circle at 1px 1px, rgba(255,255,255,0.15) 1px, transparent 0); background-size: 20px 20px;">
      <div class="absolute inset-0 bg-gradient-to-br from-neutral-900/5 to-neutral-800/10 dark:from-neutral-800/20 dark:to-neutral-700/30"></div>
      <div class="relative z-10 px-4 py-16 sm:px-6 lg:px-8 lg:py-20">
    <div class="mx-auto max-w-6xl">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-neutral-800 dark:text-neutral-200 lg:text-4xl mb-4">
          Explore Our Other Services
        </h2>
        <div class="w-24 h-1 bg-gradient-to-r from-[#D89D40] to-[#FE9002] mx-auto mb-6 rounded-full"></div>
      </div>

      <div class="grid gap-6 md:grid-cols-3">
        <!-- Business Consulting -->
        <a href="/services/business-consulting" class="group bg-white dark:bg-neutral-800 rounded-xl p-6 shadow-sm border border-gray-200 dark:border-neutral-700 hover:shadow-lg hover:border-[#FE9002]/20 dark:hover:border-[#FE9002]/20 transition-all duration-300 hover:-translate-y-1">
          <div class="w-12 h-12 bg-gradient-to-br from-[#D89D40] to-[#FE9002] rounded-xl flex items-center justify-center mb-4">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"></path>
            </svg>
          </div>
          <h3 class="text-lg font-bold text-neutral-800 dark:text-white mb-2 group-hover:text-[#FE9002] transition-colors">Business Consulting</h3>
          <p class="text-sm text-neutral-600 dark:text-neutral-400">Strategic business guidance from MBA-level consultants.</p>
        </a>

        <!-- Digital Marketing -->
        <a href="/services/digital-marketing" class="group bg-white dark:bg-neutral-800 rounded-xl p-6 shadow-sm border border-gray-200 dark:border-neutral-700 hover:shadow-lg hover:border-[#FE9002]/20 dark:hover:border-[#FE9002]/20 transition-all duration-300 hover:-translate-y-1">
